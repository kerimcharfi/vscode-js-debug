            "sourceLanguages": [
                "swift"
            ],
            "name": "Release mycode (viteHotreload/src/lib/swift)",
            "program": "${workspaceFolder:testWorkspace}/viteHotreload/src/lib/swift/.build/release/mycode",
            "args": [],
            "cwd": "${workspaceFolder:testWorkspace}/viteHotreload/src/lib/swift",
            "preLaunchTask": "swift: Build Release mycode (viteHotreload/src/lib/swift)"
        }
    ]
